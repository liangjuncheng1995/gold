<!--components/navi/navi.wxml-->
<view class='container'>
  <image bind:tap="onLeft" catch:tap="onLeft" class='icon' src="{{latest ? disLeftSrc : leftSrc}}" />
  <text class='title'>{{title}}</text>
  <image bind:tab="onRight" catch:tap="onRight" class='icon' src="{{first ? disRightSrc : rightSrc}}" />
</view>